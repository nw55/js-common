{
  "name": "@nw55/common",
  "description": "NW55 Common JS/TS Library",
  "version": "0.1.3",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Niklas Werner (nw55) <nw55@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build:esm": "tsc -p tsconfig.prod.json --outDir lib",
    "build:cjs": "tsc -p tsconfig.prod.json --outDir dist --module commonjs",
    "build:dts": "tsc -p tsconfig.prod.json --outFile lib/index.js --declaration --module umd --emitDeclarationOnly && node fix-dts-bundle.js @nw55/common lib/index.d.ts",
    "build": "npm run build:esm && npm run build:cjs && npm run build:dts",
    "dev": "tsc --watch",
    "prepare": "npm run build"
  },
  "files": [
    "/dist",
    "/lib",
    "/src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "~3.7.3"
  }
}
